{
    "manifest_version": 3,
    "name": "Spotify Now Playing",
    "version": "1.0",
    "description": "Displays the currently playing song and artist on Spotify.",
    "permissions": [
      "activeTab",
      "scripting"
    ],
    "background": {
      "service_worker": "background.js"
    },
    "action": {
      "default_popup": "popup.html"
    }
  }
  